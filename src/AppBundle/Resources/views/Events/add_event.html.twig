{% extends "::base.html.twig" %}

{% block title %}AppBundle:Events:addEvent{% endblock %}

{% block body %}
<h1>Welcome to the Events:addEvent page</h1>
    h2 class="pageheader">Ajout de Evenement</h2>

    {{form_start(form)}}
    {{form_widget(form)}}
    {{ form_row(form.nom) }}
    {{ form_row(form.description) }}
    {{ form_row(form.debut) }}
    {{ form_row(form.fin)}}

    <button type="submit">Add Event !</button>
    {{form_end(form)}}
{% endblock %}
